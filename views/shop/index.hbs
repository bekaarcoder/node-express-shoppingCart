<div class="card-deck">
	{{#each products}}
		<div class="card">
			<div class="img-thumb">
				<img src="{{imgPath}}" class="img-fluid">
			</div>
			<div class="card-body">
				<h4 class="card-title">{{title}}</h4>
				<p class="card-text"><Strong>Genre(s) : </strong>{{genre}}</p>
				<h4 class="lead">Price : Rs.{{price}}</h4>
				<a href="#" class="btn btn-primary">Buy this game</a>
			</div>
		</div>
	{{/each}}
</div>
